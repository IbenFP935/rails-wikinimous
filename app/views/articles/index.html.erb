<div class="header">
  <p><%= link_to "Wiki anonima UwU", articles_path %></p>
  <p><%= link_to "new article", new_article_path %></p>
</div>
<ul id="articles-list">
<% @articles.each do |article| %>
  <li>
    <h2><%=link_to article.title, article_path(article)%></h2>
    <p><%= article.content %></p>
  </li>


<% end %>
</ul>
